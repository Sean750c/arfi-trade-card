{
  "expo": {
    "name": "CardKing",
    "slug": "cardking",
    "version": "1.0.6",
    "orientation": "default",
    "icon": "./assets/images/logo.png",
    "scheme": "cardking",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bsdb.cardking",
      "usesAppleSignIn": true,
      "associatedDomains": ["applinks:www.cardking.ng", "applinks:cardking.ng"],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": ["fetch", "remote-notification"],
        "NSUserTrackingUsageDescription": "This data helps us provide relevant content and notifications. We respect your privacy and do not share your information without your permission.",
        "UNUserNotificationCenterDelegate": true
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#008751",
          "sounds": ["./assets/sounds/notification.mp3"],
          "mode": "production"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "We access your photo library so you can upload pictures of your gift cards for verification and selling.",
          "cameraPermission": "We use your camera to let you take photos of your gift cards when submitting them for sale or verification.",
          "faceIDPermission": "Allow $(PRODUCT_NAME) to use Face ID for authentication."
        }
      ],
      [
        "expo-apple-authentication",
        {
          "appleAuthUrl": "https://appleid.apple.com/auth/authorize",
          "appleAuthRedirectUrl": "https://appleid.apple.com/auth/callback",
          "appleAuthClientId": "com.bsdb.cardking.auth"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "notification": {
      "icon": "./assets/images/notification-icon.png",
      "color": "#008751",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "#{unread_notifications} new interactions"
    },
    "extra": {
      "router": {
        "unstable_allowRequireContext": true
      },
      "EXPO_PUBLIC_API_URL": "https://test-giftcard8-api.gcard8.com", 
      "EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID": "791594810994-0n8lvdm11k3hse26khpp5dmhq51d57c3.apps.googleusercontent.com",
      "EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID": "791594810994-bqhvquvepta7ca4de13d782blnec7p6u.apps.googleusercontent.com",
      "EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID": "791594810994-nuplk4snq0mp4tgnd98hijupn17hm9c0.apps.googleusercontent.com",
      "eas": {
        "projectId": "6e88a397-970f-4337-b705-b85048eb66b6"
      }
    },
    "android": {
      "package": "com.bsdb.cardking",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "www.cardking.ng",
              "pathPrefix": "/register"
            },
            {
              "scheme": "https",
              "host": "cardking.ng",
              "pathPrefix": "/register"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "jsEngine": "hermes",
    "owner": "chaoqunz"
  }
}