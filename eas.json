{
  "cli": {
    "version": ">= 14.4.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://test-giftcard8-api.gcard8.com"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://test-giftcard8-api.giftcard8.com"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "developmentClient": false
      },
      "env": {
        "EXPO_DEBUG": "1",
        "EXPO_PUBLIC_API_URL": "https://api.giftcard8.com"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6748598916"
      }
    }
  }
}